<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.service.mapper.DictionaryMapper">
    <select id="selectPage" resultType="com.example.service.entity.dictionary.DictionaryVO">
        SELECT * FROM sys_dictionaries
        <where>
            <if test="vo.name!=null and vo.name!=null">
                AND name LIKE CONCAT('%',#{vo.name},'%')
            </if>
            AND deleted = 0
        </where>
    </select>
</mapper>